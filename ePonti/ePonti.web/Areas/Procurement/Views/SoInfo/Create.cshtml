@model ePonti.web.Models.SOModels.NewSO

@{
    ViewBag.Title = "New Sales Order Info";
}

<div class="container-fluid">
    <div class="col-sm-12 col-lg-12 pageHeader">
        <div class="col-sm-10 col-lg-11 homeMenuText">@ViewBag.Title</div>
        <div>
            <a href="#" class="homeLinks save-main-form"><img src="~/images/Save.png" width="20"></a>
            <span><a href="javascript:history.go(-1)" class="homeLinks"><img src="~/images/Cancel.png" width="20"></a></span>
            <input type="hidden" id="so-url" value="@Url.Action("details", new {id="NEWID",nav=Request.QueryString["nav"] })" />
        </div>
    </div>
    @Html.Partial("_MainForm", Model)
</div>

@section scripts{
    <script src="@Url.Content("~/Scripts/pages/soinfo.js")" type="text/javascript"></script>
}