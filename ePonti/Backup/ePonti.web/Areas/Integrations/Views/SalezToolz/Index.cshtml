            {
                foreach (GetReportsBySiteCoID_Result item in ViewBag.CoReports)
                {
                        <tr class="item-row">
                            <td height="50"><input class="item-id" type="checkbox" data-id="@item.ViewID" /></td>
                            <td height="40"><img src="~/images/Reporting.png" width="30" data-id="@item.ViewID"></td>
                            <td>@item.Group</td>
                            <td>@item.Report</td>
                            <td>@item.Description</td>
                        </tr>
                    }
                }*@
            </table>
        </div>
    </div>
    <div class="row">
        <div class="headerText"></div>
