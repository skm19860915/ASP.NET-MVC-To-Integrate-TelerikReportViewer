@model ePonti.BOL.GetItemInfoByMasterItemID_Result

@{
    ViewBag.Title = "Edit Item Info";
    ViewBag.FormAction = "edit";

}

<div class="container body-content" id="editableView">
    <div class="col-sm-12 col-lg-12 pageHeader">
        <div class="col-sm-10 col-lg-11 homeMenuText">@ViewBag.Title</div>
        <div>
            <a href="#" class="homeLinks save-event editItem"><img src="~/images/Save.png" width="20"></a>
            <span><a onclick="closeEdit()" class="homeLinks"><img src="~/images/Cancel.png" width="20"></a></span>
        </div>
    </div>
    @Html.Partial("_MainForm", Model)
</div>




